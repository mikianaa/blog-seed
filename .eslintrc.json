{
  "extends": [
    "plugin:@typescript-eslint/recommended" // TypeScript の推奨ルール
  ],
  "plugins": ["@typescript-eslint", "prettier"], // プラグインを追加
  "rules": {
    "prettier/prettier": ["error"],              // Prettier のルールを適用
    "@typescript-eslint/explicit-function-return-type": "off", // 必要に応じてカスタマイズ
    "@typescript-eslint/no-explicit-any": "warn"
  }
}